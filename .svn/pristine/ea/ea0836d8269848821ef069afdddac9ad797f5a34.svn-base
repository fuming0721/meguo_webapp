.van-loading{
  width: 60px !important;
  height: 60px !important;
  margin: 20px 0 !important;
}
.van-swipe__indicator{
  background-color: #D1D1D1 !important;
  opacity: 0.8 !important;
  width: 12px !important;
  height: 12px !important;
}
.van-swipe__indicator--active{
  background-color: #F23C4E !important;
}
.van-swipe__indicator:not(:last-child){
  margin-right: 12px !important;
}
.van-tabbar{
  height: 100px !important;
}
.van-tabbar-item--active{
  color: @base_font_color !important;
}
::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
}
.van-tabs__wrap{
  height: 80px !important;
  border-bottom: 2px solid #f2f2f2;
}
.van-tabs{
  padding-top: 80px !important;
  position: sticky !important;
  top: 0;
  z-index: 2003 !important;
}
.cat, .subCategory, .timeBuy, .channelBox{
  .van-tabs{
    z-index: 2000 !important;
  }
  .navBar{
    z-index: 99 !important;
  }
}
.van-tab{
  font-size: 26px !important;
  line-height: 80px !important;
  color: #333 !important;
}
.van-tabs__line{
  height: 4px !important;
  border-radius: 4px !important;
  background-color: @base_font_color !important;
  bottom: 0px !important;
}
.van-tab--active{
  color: @base_font_color !important;
  font-weight: normal !important;
}
.van-list__loading-text{
  font-size: 24px !important;
  margin-left: 20px;
}
.van-list__finished-text{
  font-size: 24px !important;
  line-height: 100px !important;
  position: relative;
}

.NavBarInApp{
  padding-top: 0 !important;
}
.timeNav {
  position: sticky !important;
  top: 0;
  z-index: 1000;
  .van-tabs__wrap{
    height: 100px !important;
  }
  .van-tab{
    background-color: transparent;
  }
  .van-tabs{
    padding-top: 100px !important;
  }
}
.timeBuyTimeNav, .balanceList{
  .van-tabs__nav {
    background: -moz-linear-gradient(left, #FF8866 0%, #DD2D1A 100%);
    background: -webkit-linear-gradient(left, #FF8866 0%, #DD2D1A 100%);
    background: -o-linear-gradient(left, #FF8866 0%, #DD2D1A 100%);
    background: -ms-linear-gradient(left, #FF8866 0%, #DD2D1A 100%);
    background: linear-gradient(to right, #FF8866 0%, #DD2D1A 100%);
  }
  .van-tab--active{
    background: -moz-linear-gradient(top, #F76B1C 0%, #FAD961 100%);
    background: -webkit-linear-gradient(top, #F76B1C 0%, #FAD961 100%);
    background: -o-linear-gradient(top, #F76B1C 0%, #FAD961 100%);
    background: -ms-linear-gradient(top, #F76B1C 0%, #FAD961 100%);
    background: linear-gradient(to bottom, #F76B1C 0%, #FAD961 100%);
  }
}
.halfOffTimeNav{
  .van-tabs__nav {
    background: -moz-linear-gradient(left, #C86DD7 0%, #3023AE 100%);
    background: -webkit-linear-gradient(left, #C86DD7 0%, #3023AE 100%);
    background: -o-linear-gradient(left, #C86DD7 0%, #3023AE 100%);
    background: -ms-linear-gradient(left, #C86DD7 0%, #3023AE 100%);
    background: linear-gradient(to right, #C86DD7 0%, #3023AE 100%);
  }
  .van-tab--active{
    background: -moz-linear-gradient(top, #C86DD7 0%, #7243BF 100%);
    background: -webkit-linear-gradient(top, #C86DD7 0%, #7243BF 100%);
    background: -o-linear-gradient(top, #C86DD7 0%, #7243BF 100%);
    background: -ms-linear-gradient(top, #C86DD7 0%, #7243BF 100%);
    background: linear-gradient(to bottom, #C86DD7 0%, #7243BF 100%);
  }
}
.van-notify{
  font-size: 26px !important;
  height: 60px !important;
  line-height: 60px !important;
  padding: 0 !important;
}
.van-cell{
  padding: 20px 30px !important;
  font-size: 26px !important;
  line-height: 48px !important;
}
.van-cell__right-icon{
  height: 48px !important;
  font-size: 32px !important;
  line-height: 48px !important;
}
.van-cell:not(:last-child)::after {
  border-bottom: 2px solid #f2f2f2 !important;
}
.van-tabs__nav--line{
  padding-bottom: 0px !important;
}
.headerNavB_center{
  .van-tabs__nav{
    height: 80px !important;
  }
  .van-tabs__wrap{
    overflow: inherit !important;
  }
  .van-tabs__line{
    bottom: 0 !important;
  }
}
.bannerMain{
  .van-swipe__indicators {
    bottom: 20px !important;
  }
}
.my_video.vjs-custom-skin > .video-js {
  .vjs-big-play-button{
    height: 120px !important;
    width: 120px;
    border-radius: 50%;
    font-size: 58px;
    margin-left: -60px;
  }
  .vjs-control-bar{
    height: 80px;
  }
  .vjs-control{
    width: 60px;
    font-size: 28px;
    padding: 0;
  }
  .vjs-mute-control{
    font-size: 20px;
  }
  .vjs-icon-placeholder:before{
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .vjs-time-control{
    font-size: 22px;
    line-height:80px;
    padding: 0;
  }
  .vjs-slider-bar:before{
    border-radius: 50%;
    content: '';
    width: 24px;
    height: 24px;
    background-color: #fff;
  }
  .vjs-playback-rate-value{
    font-size: 23px !important;
    display: flex;
    justify-content: center;
    align-items: center;
  }
}
.zhongCao {
  .van-tabs__line {
    bottom: 0 !important;
  }
  .van-tabs__nav{
    background-color: #5D20E4;
  }
  .navItemName{
    background-color: #5D20E4;
    color: #fff;
  }
  .van-tab{
    padding: 0;
  }
  .van-tab--active{
    color: #fff !important;
  }
  .van-tabs__line{
    background-color: #fff !important;
  }
}
.van-image-preview__index{
  font-size: 28px !important;
}
.loginloading{
  width: 30px !important;
  margin: 0 !important;
  margin-right: 20px !important;
}
.my{
  .van-cell{
    height: 100px !important;
    line-height: 60px !important;
  }
}
.myDialog, .des{
  height: 300px !important;
  width: 600px !important;
  border-radius: 16px !important;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  padding: 20px !important;
  .van-dialog__header{
    font-size: 32px;
    font-weight: bolder;
    padding: 0 0 20px !important;
  }
  .van-dialog__footer{
    width: 100% !important;
    position: static !important;
    height: 72px !important;
    .van-dialog__cancel{
      position: absolute !important;
      top: 8px !important;
      right: 8px !important;
      width: 36px !important;
      height: 36px !important;
      color: #fff;
      text-align: center !important;
      font-size: 28px !important;
      transform:rotate(45deg);
      line-height: 36px;
      border-radius: 36px;
      background-color: rgba(0, 0, 0, 0.5);
    }
  }
  .van-dialog__content{
    flex-grow: 1;
    .van-dialog__message{
      height: 100% !important;
      width: 100% !important;
      font-size: 24px !important;
      color: #4a4a4a;
      padding: 20px !important;
    }
  }
  .van-dialog__confirm{
    width: 100% !important;
    height: 100% !important;
    border-radius: 8px !important;
    color: #fff !important;
    font-size: 30px !important;
    background-color: #1AAD19 !important;
  }
}
.des{
  height: 240px !important;
  .van-dialog__footer{
    height: 0 !important;
  }
}
.balanceList{
  .van-tabs__nav{
    height: 100px !important;
  }
  .van-tabs__line{
    bottom: 2px !important;
  }
  .van-tabs__wrap{
    height: 100px !important;
  }
  .van-tab{
    background-color: #FF740E !important;
  }
  .van-tabs{
    padding-top: 100px !important;
  }
}
.orderdetail{
  .navBar{
    z-index: 2100 !important;
  }
}
.orderdetail, .team{
  .van-popup{
    top: 80px !important;
  }
  .van-tabs{
    padding-top: 100px !important;
    z-index: 1000 !important;
  }
  .van-tabs__wrap {
    height: 100px !important;
  }
  .van-tabs__line{
    bottom: 0px !important;
  }
  .van-ellipsis{
    height: 100% !important;
    line-height: 98px !important;
  }
}
.team {
  .van-tabs{
    padding-top: 100px !important;
    z-index: 2030 !important;
  }
}
.logout{
  .van-dialog__message{
    font-size: 26px !important;
    padding: 50px;
    text-align: center;
  }
  .van-button{
    height: 80px !important;
    font-size: 24px !important;
  }
}
.searchTop{
  .van-tabs{
    z-index: 0 !important;
  }
}
.searchOverLay{
  background-color: rgba(0, 0, 0, 0.4) !important;
}
.coupon>.van-switch-cell{
  padding: 0 30px !important;
  height: 100%;
  color: 28px;
}
.deleteItem{
  .van-checkbox__icon{
    font-size: 24px !important;
    height: 30px !important;
    line-height: 30px !important;
    width: 30px !important;
    .van-icon-success{
      font-size: 24px;
      width: 30px;
      height: 30px;
    }
  }
}
